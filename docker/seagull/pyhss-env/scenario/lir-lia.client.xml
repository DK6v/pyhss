<?xml version="1.0" encoding="ISO-8859-1" ?>
<scenario>

<counter>
  <counterdef name="session-counter" init="0"> </counterdef>
  <counterdef name="imsi-counter" init="250800000000001" max="2508000000000100" behavior="init"></counterdef>
</counter>

<!-- Traffic -->
<traffic>
  <send channel="channel-1">
    <action>
      <!-- For each new call, increment the session-ID counter -->
      <inc-counter name="session-counter"> </inc-counter>
      <inc-counter name="imsi-counter"> </inc-counter>
      <set-value name="Session-Id" 
                 format=".;1096298391;$(session-counter)"></set-value>
      <set-value name="Vendor-Public-Identity" 
                 format="sip:$(imsi-counter)"></set-value>
    </action>
    <command name="LIR">
      <avp name="Session-Id" value="value_is_replaced"> </avp>
      <avp name="Auth-Session-State" value="0"> </avp>
      <avp name="Origin-Host" value="seagull"> </avp>
      <avp name="Origin-Realm" value="seagull"> </avp>
      <avp name="Destination-Realm" value="Cx"> </avp>
      <avp name="Vendor-Public-Identity" value="value_is_replaced"> </avp>
    </command>
    <action>
      <start-timer></start-timer>
    </action>
  </send>
  
  <receive channel="channel-1">
    <action>
      <stop-timer></stop-timer>
    </action>
    <command name="LIA">
    </command>
  </receive>
</traffic>
  
</scenario>
